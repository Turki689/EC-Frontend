{"ast":null,"code":"import axios from \"axios\";\nimport Product from \"../components/Product.vue\";\nexport default {\n  name: \"ProductList\",\n  components: {\n    Product\n  },\n  data() {\n    return {\n      products: [],\n      errorMsg: \"\",\n      displayedProducts: [],\n      name: \"Products\",\n      filter: \"\",\n      page: 1,\n      showy: false\n    };\n  },\n  computed: {\n    filteredProducts() {\n      if (this.filter === \"\") {\n        return this.displayedProducts;\n      }\n      return this.products.filter(product => {\n        return product.title.toLowerCase().startsWith(this.filter.toLowerCase());\n      });\n    },\n    hasMoreProducts() {\n      return this.products.length > this.displayedProducts.length;\n    }\n  },\n  methods: {\n    getProducts() {\n      let url = \"http://localhost:8000/products/\";\n      axios.get(url).then(response => {\n        this.products = response.data;\n        this.displayedProducts = this.products.slice(0, 24);\n      }).catch(error => {\n        this.errorMsg = `Error retrieving data: ${error.message}`;\n      });\n    },\n    showMoreProducts() {\n      const nextProducts = this.products.slice(this.displayedProducts.length, this.displayedProducts.length + 24);\n      if (nextProducts.length > 0) {\n        this.displayedProducts = this.displayedProducts.concat(nextProducts);\n        this.page++;\n      }\n    },\n    showe() {\n      this.showy = !this.showy;\n    }\n  },\n  created() {\n    this.getProducts();\n  },\n  mounted() {\n    this.getProducts();\n  }\n};","map":{"version":3,"names":["axios","Product","name","components","data","products","errorMsg","displayedProducts","filter","page","showy","computed","filteredProducts","product","title","toLowerCase","startsWith","hasMoreProducts","length","methods","getProducts","url","get","then","response","slice","catch","error","message","showMoreProducts","nextProducts","concat","showe","created","mounted"],"sources":["/Users/turki/Documents/GitHub/g-project/src/views/Prodects.vue"],"sourcesContent":["<template>\r\n    <section>\r\n        <h1 class=\"header\">This is {{ name }} page</h1>\r\n        <div class=\"container-fluid featurs py-5\">\r\n            <div class=\"container py-5\">\r\n                <div class=\"tab-class text-center\">\r\n                    <div class=\"tab-content\">\r\n                        <div class=\"col-lg-12\">\r\n                            <div class=\"input-group mt-4\">\r\n                                <div class=\"search-box\">\r\n                                    <input class=\"form-control me-2\" type=\"search\" placeholder=\"Search\" aria-label=\"Search\"\r\n                                        v-model=\"filter\" v-show=\"showy\" />\r\n                                    <font-awesome-icon icon=\"fa-search\" class=\"icon\" @click=\"showe\" />\r\n                                </div>\r\n                            </div>\r\n                            <div class=\"row g-4\">\r\n                                <h3 v-if=\"errorMsg\">{{ errorMsg }}</h3>\r\n                                <Product v-for=\"product in filteredProducts\" :key=\"product.id\" :product=\"product\" />\r\n                            </div>\r\n                            <button v-if=\"hasMoreProducts\" @click=\"showMoreProducts\" class=\"btn btn-primary mt-4\">\r\n                                More\r\n                            </button>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </section>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport Product from \"../components/Product.vue\";\r\n\r\nexport default {\r\n    name: \"ProductList\",\r\n    components: {\r\n        Product,\r\n    },\r\n    data() {\r\n        return {\r\n            products: [],\r\n            errorMsg: \"\",\r\n            displayedProducts: [],\r\n            name: \"Products\",\r\n            filter: \"\",\r\n            page: 1,\r\n            showy: false,\r\n        };\r\n    },\r\n    computed: {\r\n        filteredProducts() {\r\n            if (this.filter === \"\") {\r\n                return this.displayedProducts;\r\n            }\r\n            return this.products.filter((product) => {\r\n                return product.title\r\n                    .toLowerCase()\r\n                    .startsWith(this.filter.toLowerCase());\r\n            });\r\n        },\r\n        hasMoreProducts() {\r\n            return this.products.length > this.displayedProducts.length;\r\n        },\r\n    },\r\n    methods: {\r\n        getProducts() {\r\n            let url = \"http://localhost:8000/products/\";\r\n            axios\r\n                .get(url)\r\n                .then((response) => {\r\n                    this.products = response.data;\r\n                    this.displayedProducts = this.products.slice(0, 24);\r\n                })\r\n                .catch((error) => {\r\n                    this.errorMsg = `Error retrieving data: ${error.message}`;\r\n                });\r\n        },\r\n        showMoreProducts() {\r\n            const nextProducts = this.products.slice(\r\n                this.displayedProducts.length,\r\n                this.displayedProducts.length + 24\r\n            );\r\n            if (nextProducts.length > 0) {\r\n                this.displayedProducts = this.displayedProducts.concat(nextProducts);\r\n                this.page++;\r\n            }\r\n        },\r\n        showe() {\r\n            this.showy = !this.showy;\r\n        },\r\n    },\r\n    created() {\r\n        this.getProducts();\r\n    },\r\n    mounted() {\r\n        this.getProducts();\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\nsection {\r\n    h1 {\r\n        margin-top: 0;\r\n        margin-bottom: 0.5rem;\r\n        font-family: \"Raleway\", sans-serif;\r\n        font-weight: 600;\r\n        line-height: 1.2;\r\n        colo