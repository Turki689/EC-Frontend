{"ast":null,"code":"import axios from \"axios\";\nimport Product from \"../components/Product.vue\";\nexport default {\n  name: \"ProductList\",\n  components: {\n    Product\n  },\n  data() {\n    return {\n      products: [],\n      errorMsg: \"\",\n      displayedProducts: [],\n      name: \"Products\",\n      filter: \"\",\n      page: 1,\n      showy: false\n    };\n  },\n  computed: {\n    filteredProducts() {\n      if (this.filter === \"\") {\n        return this.displayedProducts;\n      }\n      return this.products.filter(product => {\n        return product.name.toLowerCase().startsWith(this.filter.toLowerCase());\n      });\n    },\n    hasMoreProducts() {\n      return this.products.length > this.displayedProducts.length;\n    }\n  },\n  methods: {\n    getProducts() {\n      let url = \"http://127.0.0.1:8000/products/\";\n      axios.get(url).then(response => {\n        this.products = response.data;\n        console.log(response.data);\n        this.displayedProducts = this.products.slice(0, 24);\n      }).catch(error => {\n        console.log(error);\n        this.errorMsg = \"Error retrieving data\";\n      });\n    },\n    showMoreProducts() {\n      const nextProducts = this.products.slice(this.displayedProducts.length, this.displayedProducts.length + 24);\n      if (nextProducts.length > 0) {\n        this.displayedProducts = this.displayedProducts.concat(nextProducts);\n        this.page++;\n      }\n    },\n    showe() {\n      if (this.showy == false) {\n        this.showy = true;\n      } else {\n        this.showy = false;\n      }\n    }\n  },\n  created() {\n    this.getProducts();\n  },\n  mounted() {\n    this.getProducts();\n  }\n};","map":{"version":3,"names":["axios","Product","name","components","data","products","errorMsg","displayedProducts","filter","page","showy","computed","filteredProducts","product","toLowerCase","startsWith","hasMoreProducts","length","methods","getProducts","url","get","then","response","console","log","slice","catch","error","showMoreProducts","nextProducts","concat","showe","created","mounted"],"sources":["/Users/turki/Documents/GitHub/g-project/src/views/Prodects.vue"],"sourcesContent":["<!-- eslint-disable prettier/prettier -->\r\n<template>\r\n    <section>\r\n        <h1 class=\"header\">This is {{ name }} page</h1>\r\n        <div class=\"container-fluid featurs py-5\">\r\n            <div class=\"container py-5\">\r\n                <div class=\"tab-class text-center\">\r\n                    <div class=\"tab-content\">\r\n                        <div class=\"col-lg-12\">\r\n                            <div class=\"input-group mt-4\">\r\n                                <div class=\"search-box\">\r\n                                    <input\r\n                                        class=\"form-control me-2\"\r\n                                        type=\"search\"\r\n                                        placeholder=\"Search\"\r\n                                        aria-label=\"Search\"\r\n                                        v-model=\"filter\"\r\n                                        v-show=\"showy\"\r\n                                    />\r\n                                    <font-awesome-icon\r\n                                        icon=\"fa-search\"\r\n                                        class=\"icon\"\r\n                                        @click=\"showe\"\r\n                                    />\r\n                                </div>\r\n                            </div>\r\n                            <div class=\"row g-4\">\r\n                                <h3 v-if=\"errorMsg\">{{ errorMsg }}</h3>\r\n                                <Product\r\n                                    v-for=\"(product,i) in filteredProducts\"\r\n                                    :key=\"i\"\r\n                                    :data = \"product\"\r\n                                />\r\n                            </div>\r\n                            <button\r\n                                v-if=\"hasMoreProducts\"\r\n                                @click=\"showMoreProducts\"\r\n                                class=\"btn btn-primary mt-4\"\r\n                            >\r\n                                More\r\n                            </button>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </section>\r\n</template>\r\n<!-- eslint-disable prettier/prettier -->\r\n<script>\r\nimport axios from \"axios\";\r\nimport Product from \"../components/Product.vue\";\r\n\r\nexport default {\r\n    name: \"ProductList\",\r\n    components: {\r\n        Product,\r\n    },\r\n    data() {\r\n        return {\r\n            products: [],\r\n            errorMsg: \"\",\r\n            displayedProducts: [],\r\n            name: \"Products\",\r\n            filter: \"\",\r\n            page: 1,\r\n            showy: false,\r\n        };\r\n    },\r\n    computed: {\r\n        filteredProducts() {\r\n            if (this.filter === \"\") {\r\n                return this.displayedProducts;\r\n            }\r\n            return this.products.filter((product) => {\r\n                return product.name\r\n                    .toLowerCase()\r\n                    .startsWith(this.filter.toLowerCase());\r\n            });\r\n        },\r\n        hasMoreProducts() {\r\n            return this.products.length > this.displayedProducts.length;\r\n        },\r\n    },\r\n    methods: {\r\n        getProducts() {\r\n            let url = \"http://127.0.0.1:8000/products/\";\r\n            axios\r\n                .get(url)\r\n                .then((response) => {\r\n                    this.products = response.data;\r\n                    console.log(response.data);\r\n                    this.displayedProducts = this.products.slice(0, 24);\r\n                })\r\n                .catch((error) => {\r\n                    console.log(error);\r\n                    this.errorMsg = \"Error retrieving data\";\r\n                });\r\n        },\r\n        showMoreProducts() {\r\n            const nextProducts = this.products.slice(\r\n                this.displayedProducts.length,\r\n                this.displayedProducts.length + 24\r\n            );\r\n            if (nextProducts.length > 0) {\r\n                this.displayedProducts =\r\n                    this.displayedProducts.concat(nextProducts);\r\n                this.page++;\r\n            }\r\n        },\r\n        showe() {\r\n            if (this.showy == false) {\r\n                this.showy = true;\r\n            } else {\r\n                this.showy = false;\r\n            }\r\n        },\r\n    },\r\n    created() {\r\n        this.getProducts();\r\n    },\r\n    mounted() {\r\n        this.getProducts();\r\n    },\r\n};\r\n</script>\r\n<!-- eslint-disable prettier/prettier -->\r\n<style scoped lang=\"scss\">\r\nsection {\r\n    h1 {\r\n        margin-top: 0;\r\n        margin-bottom: 0.5rem;\r\n        font-family: \"Raleway\", sans-serif;\r\n        font-weight: 600;\r\n        line-height: 1.2;\r\n        color: #45595b;\r\n    }\r\n\r\n    background-color: rgb(255 255 255);\r\n\r\n    .container {\r\n        background-color: rgb(255 255 255);\r\n    }\r\n    .search-box {\r\n        padding-bottom: 20px;\r\n    }\r\n    .icon {\r\n        height: 30px;\r\n        width: 40px;\r\n        color: #81c408 !important;\r\n        padding-left: 20px;\r\n        cursor: pointer;\r\n        position: absolute;\r\n        right: 0;\r\n        top: -60px;\r\n    }\r\n    .icon:hover {\r\n    color: var(--bs-nav-link-color) !important;\r\n}\r\n    input {\r\n        padding: 10px;\r\n        position: absolute;\r\n        top: -60px;\r\n        height: 30px;\r\n        width: 200px;\r\n        right: 40px;\r\n    }\r\n\r\n    .btn {\r\n        &.btn-primary {\r\n            background-color: rgb(19 120 204);\r\n            border-color: rgb(19 120 204);\r\n            color: white;\r\n            padding: 0.5rem 1rem;\r\n            border-radius: 0.25rem;\r\n            transition: all 0.2s ease-in-out;\r\n\r\n            &:hover {\r\n                background-color: rgb(10 78 153);\r\n                border-color: rgb(10 78 153);\r\n            }\r\n\r\n            & :focus {\r\n                box-shadow: 0 0 0 0.2rem rgb(12 52 102 / 50%);\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n"],"mappings":"AAkDA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,OAAM,MAAO,2BAA2B;AAE/C,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,iBAAiB,EAAE,EAAE;MACrBL,IAAI,EAAE,UAAU;MAChBM,MAAM,EAAE,EAAE;MACVC,IAAI,EAAE,CAAC;MACPC,KAAK,EAAE;IACX,CAAC;EACL,CAAC;EACDC,QAAQ,EAAE;IACNC,gBAAgBA,CAAA,EAAG;MACf,IAAI,IAAI,CAACJ,MAAK,KAAM,EAAE,EAAE;QACpB,OAAO,IAAI,CAACD,iBAAiB;MACjC;MACA,OAAO,IAAI,CAACF,QAAQ,CAACG,MAAM,CAAEK,OAAO,IAAK;QACrC,OAAOA,OAAO,CAACX,IAAG,CACbY,WAAW,CAAC,EACZC,UAAU,CAAC,IAAI,CAACP,MAAM,CAACM,WAAW,CAAC,CAAC,CAAC;MAC9C,CAAC,CAAC;IACN,CAAC;IACDE,eAAeA,CAAA,EAAG;MACd,OAAO,IAAI,CAACX,QAAQ,CAACY,MAAK,GAAI,IAAI,CAACV,iBAAiB,CAACU,MAAM;IAC/D;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,WAAWA,CAAA,EAAG;MACV,IAAIC,GAAE,GAAI,iCAAiC;MAC3CpB,KAAI,CACCqB,GAAG,CAACD,GAAG,EACPE,IAAI,CAAEC,QAAQ,IAAK;QAChB,IAAI,CAAClB,QAAO,GAAIkB,QAAQ,CAACnB,IAAI;QAC7BoB,OAAO,CAACC,GAAG,CAACF,QAAQ,CAACnB,IAAI,CAAC;QAC1B,IAAI,CAACG,iBAAgB,GAAI,IAAI,CAACF,QAAQ,CAACqB,KAAK,CAAC,CAAC,EAAE,EAAE,CAAC;MACvD,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QACdJ,OAAO,CAACC,GAAG,CAACG,KAAK,CAAC;QAClB,IAAI,CAACtB,QAAO,GAAI,uBAAuB;MAC3C,CAAC,CAAC;IACV,CAAC;IACDuB,gBAAgBA,CAAA,EAAG;MACf,MAAMC,YAAW,GAAI,IAAI,CAACzB,QAAQ,CAACqB,KAAK,CACpC,IAAI,CAACnB,iBAAiB,CAACU,MAAM,EAC7B,IAAI,CAACV,iBAAiB,CAACU,MAAK,GAAI,EACpC,CAAC;MACD,IAAIa,YAAY,CAACb,MAAK,GAAI,CAAC,EAAE;QACzB,IAAI,CAACV,iBAAgB,GACjB,IAAI,CAACA,iBAAiB,CAACwB,MAAM,CAACD,YAAY,CAAC;QAC/C,IAAI,CAACrB,IAAI,EAAE;MACf;IACJ,CAAC;