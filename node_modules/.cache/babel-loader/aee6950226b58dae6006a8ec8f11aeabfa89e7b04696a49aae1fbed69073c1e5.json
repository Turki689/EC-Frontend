                     @click=\"showCompany(company)\"\n                                />\n                            </div>\n                            <button\n                                v-if=\"hasMoreCompanies\"\n                                @click=\"showMoreCompanies\"\n                                class=\"btn btn-primary mt-4\"\n                            >\n                                More\n                            </button>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </section>\n</template>\n<!-- eslint-disable prettier/prettier -->\n<script>\nimport companiesView from \"../components/companiesView.vue\";\nimport axios from \"axios\";\nexport default {\n    name: \"ComPanies\",\n    components: {\n        companiesView,\n    },\n    data() {\n        return {\n            Companies: [],\n            company: {},\n            displayedCompanies: [],\n            name: \"  The  companies \",\n            filter: \"\",\n            page: 1,\n            showy: false,\n            errorMsg: \"\",\n        };\n    },\n    computed: {\n        filteredCompanies() {\n            if (this.filter === \"\") {\n                return this.displayedCompanies;\n            }\n            return this.Companies.filter((company) => {\n                return company.name\n                    .toLowerCase()\n                    .startsWith(this.filter.toLowerCase());\n            });\n        },\n\n        hasMoreCompanies() {\n            return this.Companies.length > this.displayedCompanies.length;\n        },\n    },\n    methods: {\n        getCompanies() {\n            let url = \"https://api.example.com/company\";\n            axios\n                .get(url)\n                .then((response) => {\n                    this.Companies = response.data;\n                    console.log(response.data);\n                    this.displayedCompanies = this.Companies.slice(0, 16);\n                })\n                .catch((error) => {\n                    console.log(error);\n                    this.errorMsg = \"Error retrieving data\";\n                });\n        },\n        showMoreCompanies() {\n            const nextCompanies = this.Companies.slice(\n                this.displayedCompanies.length,\n                this.displayedCompanies.length + 16\n            );\n            if (nextCompanies.length > 0) {\n                this.displayedCompanies =\n                    this.displayedCompanies.concat(nextCompanies);\n                this.page++;\n            }\n        },\n        showcompany(id) {\n            let allcompanies = this.Companies;\n            let company = allcompanies.filter((company) => company.id == id);\n            this.company = company;\n        },\n        showe() {\n            if (this.showy == false) {\n                this.showy = true;\n            } else {\n                this.showy = false;\n            }\n        },\n    },\n    // eslint-disable-next-line vue/multi-word-component-names\n    created() {\n        this.getCompanies();\n    },\n    mounted() {\n        this.getCompanies();\n    },\n};\n</script>\n<!-- eslint-disable prettier/prettier -->\n<style scoped lang=\"scss\">\nsection {\n    h1 {\n        margin-top: 0;\n        margin-bottom: 0.5rem;\n        font-family: \"Raleway\", sans-serif;\n        font-weight: 600;\n        line-height: 1.2;\n        color: #45595b;\n    }\n\n    background-color: rgb(255 255 255);\n    .container {\n        background-color: rgb(255 255 255);\n    }\n    .search-box {\n        padding-bottom: 20px;\n    }\n    .icon {\n        height: 30px;\n        width: 40px;\n        color: #81c408 !important;\n        padding-left: 20px;\n        cursor: pointer;\n        position: absolute;\n        right: 0;\n        top: -60px;\n    }\n    .icon:hover {\n    color: var(--bs-nav-link-color) !important;\n}\n    input {\n        padding: 10px;\n        position: absolute;\n        top: -60px;\n        height: 30px;\n        width: 200px;\n        right: 40px;\n    }\n    .btn {\n        &.btn-primary {\n            background-color: rgb(19 120 204);\n            border-color: rgb(19 120 204);\n            color: white;\n            padding: 0.5rem 1rem;\n            border-radius: 0.25rem;\n            transition: all 0.2s ease-in-out;\n            &:hover {\n                background-color: rgb(10 78 153);\n                border-color: rgb(10 78 153);\n            }\n            &:focus {\n                box-shadow: 0 0 0 0.2rem rgb(12 52 102 / 50%);\n            }\n        }\n    }\n}\n</style>\n"],"mappings":";;;EAGYA,KAAK,EAAC;AAAO;;EACZA,KAAK,EAAC;AAA8B;;EAChCA,KAAK,EAAC;AAAgB;;EAClBA,KAAK,EAAC;AAAuB;;EACzBA,KAAK,EAAC;AAAa;;EACfA,KAAK,EAAC;AAAW;;EACbA,KAAK,EAAC;AAAkB;;EACpBA,KAAK,EAAC;AAAY;;EAgBtBA,KAAK,EAAC;AAAS;;EA1BhDC,GAAA;AAAA;;;;uBAEIC,mBAAA,CAgDU,kBA/CNC,mBAAA,CAAmD,MAAnDC,UAAmD,EAAjC,UAAQ,GAAAC,gBAAA,CAAGC,KAAA,CAAAC,IAAI,UAAS,MAAI,iBAC9CJ,mBAAA,CA6CM,OA7CNK,UA6CM,GA5CFL,mBAAA,CA2CM,OA3CNM,UA2CM,GA1CFN,mBAAA,CAyCM,OAzCNO,UAyCM,GAxCFP,mBAAA,CAuCM,OAvCNQ,UAuCM,GAtCFR,mBAAA,CAqCM,OArCNS,UAqCM,GApCFT,mBAAA,CAgBM,OAhBNU,UAgBM,GAfFV,mBAAA,CAcM,OAdNW,UAcM,G,gBAbFX,mBAAA,CAOE;IANEH,KAAK,EAAC,mBAAmB;IACzBe,IAAI,EAAC,QAAQ;IACbC,WAAW,EAAC,QAAQ;IACpB,YAAU,EAAC,QAAQ;IAf3D,uBAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAgBiDZ,KAAA,CAAAa,MAAM,GAAAD,MAAA;iDAANZ,KAAA,CAAAa,MAAM,G,SACPb,KAAA,CAAAc,KAAK,E,GAEjBC,YAAA,CAIEC,4BAAA;IAHEC,IAAI,EAAC,WAAW;IAChBvB,KAAK,EAAC,MAAM;IACXwB,OAAK,EAAEC,QAAA,CAAAC;4CAIpBvB,mBAAA,CAWM,OAXNwB,UAWM,GAVQrB,KAAA,CAAAsB,QAAQ,I,cAAlB1B,mBAAA,CAAuC,MA3BvE2B,WAAA,EAAAxB,gBAAA,CA2BuDC,KAAA,CAAAsB,QAAQ,oBA3B/DE,mBAAA,iB,kBA4BgC5B,mBAAA,CAQE6B,SAAA,QApClCC,WAAA,CA6BsDP,QAAA,CAAAQ,iBAAiB,EAA5BC,OAAO;yBADlBC,YAAA,CAQEC,wBAAA;MANGnC,GAAG,EAAEiC,OAAO,CAACG,EAAE;MACf9B,IAAI,EAAE2B,OAAO,CAAC3B,IAAI;MAClB+B,WAAW,EAAEJ,OAAO,CAACI,WAAW;MAChCC,IAAI,EAAEL,OAAO,CAACK,IAAI;MAClBC,KAAK,EAAEN,OAAO,CAACM,KAAK;MACpBhB,OAAK,EAAAN