                           </div>\r\n                            <button\r\n                                v-if=\"hasMoreCompanies\"\r\n                                @click=\"showMoreCompanies\"\r\n                                class=\"btn btn-primary mt-4\"\r\n                            >\r\n                                More\r\n                            </button>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </section>\r\n</template>\r\n<!-- eslint-disable prettier/prettier -->\r\n<script>\r\nimport companiesView from \"../components/companiesView.vue\";\r\nimport axios from \"axios\";\r\nexport default {\r\n    name: \"ListOfCompanies\",\r\n    components: {\r\n        companiesView,\r\n    },\r\n    data() {\r\n        return {\r\n            Companies: [],\r\n            displayedCompanies: [],\r\n            name: \"  List of companies\",\r\n            filter: \"\",\r\n            page: 1,\r\n            showy: false,\r\n        };\r\n    },\r\n\r\n    computed: {\r\n        filteredCompanies() {\r\n            if (this.filter === \"\") {\r\n                return this.displayedCompanies;\r\n            }\r\n            return this.Companies.filter((company) => {\r\n                return company.name\r\n                    .toLowerCase()\r\n                    .startsWith(this.filter.toLowerCase());\r\n            });\r\n        },\r\n        hasMoreCompanies() {\r\n            return this.Companies.length > this.displayedCompanies.length;\r\n        },\r\n    },\r\n\r\n    methods: {\r\n        getCompanies() {\r\n            let url = \"http://127.0.0.1:8000/companies/\";\r\n            axios\r\n                .get(url)\r\n                .then((response) => {\r\n                    this.Companies = response.data;\r\n                    console.log(response.data);\r\n                    this.displayedCompanies = this.Companies.slice(0, 24);\r\n                })\r\n                .catch((error) => {\r\n                    console.log(error);\r\n                    this.errorMsg = \"Error retrieving data\";\r\n                });\r\n        },\r\n        showMoreCompanies() {\r\n            const nextCompanies = this.Companies.slice(\r\n                this.displayedCompanies.length,\r\n                this.displayedCompanies.length + 24\r\n            );\r\n            if (nextCompanies.length > 0) {\r\n                this.displayedCompanies =\r\n                    this.displayedCompanies.concat(nextCompanies);\r\n                this.page++;\r\n            }\r\n        },\r\n        showe() {\r\n            if (this.showy == false) {\r\n                this.showy = true;\r\n            } else {\r\n                this.showy = false;\r\n            }\r\n        },\r\n    },\r\n    // eslint-disable-next-line vue/multi-word-component-names\r\n    created() {\r\n        this.getCompanies();\r\n    },\r\n    mounted() {\r\n        this.getCompanies();\r\n    },\r\n};\r\n</script>\r\n<!-- eslint-disable prettier/prettier -->\r\n<style scoped lang=\"scss\">\r\nsection {\r\n    h1 {\r\n        margin-top: 0;\r\n        margin-bottom: 0.5rem;\r\n        font-family: \"Raleway\", sans-serif;\r\n        font-weight: 600;\r\n        line-height: 1.2;\r\n        color: #45595b;\r\n    }\r\n\r\n    background-color: rgb(255 255 255);\r\n    .container {\r\n        background-color: rgb(255 255 255);\r\n    }\r\n    .search-box {\r\n        padding-bottom: 20px;\r\n    }\r\n    .icon {\r\n        height: 30px;\r\n        width: 40px;\r\n        color: #81c408 !important;\r\n        padding-left: 20px;\r\n        cursor: pointer;\r\n        position: absolute;\r\n        right: 0;\r\n        top: -60px;\r\n    }\r\n    .icon:hover {\r\n        color: var(--bs-nav-link-color) !important;\r\n    }\r\n    input {\r\n        padding: 10px;\r\n        position: absolute;\r\n        top: -60px;\r\n        height: 30px;\r\n        width: 200px;\r\n        right: 40px;\r\n    }\r\n    .btn {\r\n        &.btn-primary {\r\n            background-color: rgb(19 120 204);\r\n            border-color: rgb(19 120 204);\r\n            color: white;\r\n            padding: 0.5rem 1rem;\r\n            border-radius: 0.25rem;\r\n            transition: all 0.2s ease-in-out;\r\n            &:hover {\r\n                background-color: rgb(10 78 153);\r\n                border-color: rgb(10 78 153);\r\n            }\r\n            &:focus {\r\n                box-shadow: 0 0 0 0.2rem rgb(12 52 102 / 50%);\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>\r\n\r\n"],"mappings":"AAiDA,OAAOA,aAAY,MAAO,iCAAiC;AAC3D,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACXC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACRH;EACJ,CAAC;EACDI,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE,EAAE;MACbC,kBAAkB,EAAE,EAAE;MACtBJ,IAAI,EAAE,qBAAqB;MAC3BK,MAAM,EAAE,EAAE;MACVC,IAAI,EAAE,CAAC;MACPC,KAAK,EAAE;IACX,CAAC;EACL,CAAC;EAEDC,QAAQ,EAAE;IACNC,iBAAiBA,CAAA,EAAG;MAChB,IAAI,IAAI,CAACJ,MAAK,KAAM,EAAE,EAAE;QACpB,OAAO,IAAI,CAACD,kBAAkB;MAClC;MACA,OAAO,IAAI,CAACD,SAAS,CAACE,MAAM,CAAEK,OAAO,IAAK;QACtC,OAAOA,OAAO,CAACV,IAAG,CACbW,WAAW,CAAC,EACZC,UAAU,CAAC,IAAI,CAACP,MAAM,CAACM,WAAW,CAAC,CAAC,CAAC;MAC9C,CAAC,CAAC;IACN,CAAC;IACDE,gBAAgBA,CAAA,EAAG;MACf,OAAO,IAAI,CAACV,SAAS,CAACW,MAAK,GAAI,IAAI,CAACV,kBAAkB,CAACU,MAAM;IACjE;EACJ,CAAC;EAEDC,OAAO,EAAE;IACLC,YAAYA,CAAA,EAAG;MACX,IAAIC,GAAE,GAAI,kCAAkC;MAC5ClB,KAAI,CACCmB,GAAG,CAACD,GAAG,EACPE,IAAI,CAAEC,QAAQ,IAAK;QAChB,IAAI,CAACjB,SAAQ,GAAIiB,QAAQ,CAAClB,IAAI;QAC9BmB,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAClB,IAAI,CAAC;QAC1B,IAAI,CAACE,kBAAiB,GAAI,IAAI,CAACD,SAAS,CAACoB,KAAK,CAAC,CAAC,EAAE,EAAE,CAAC;MACzD,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QACdJ,OAAO,CAACC,GAAG,CAACG,KAAK,CAAC;QAClB,IAAI,CAACC,QAAO,GAAI,uBAAuB;MAC3C,CAAC,CAAC;IACV,CAAC;IACDC,iBAAiBA,CAAA,EAAG;MAChB,MAAMC,aAAY,GAAI,IAAI,CAACzB,SAAS,CAACoB,KAAK,CACtC,IAAI,CAACnB,kBAAkB,CAACU,MAAM,EAC9B,IAAI,CAACV,kBAAkB,CAACU,MAAK,GAAI,EACrC,CAAC;MACD,IAAIc,aAAa,CAACd,MAAK,GAAI,C